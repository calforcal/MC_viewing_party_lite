<%= button_to 'Discover Page', discover_index_path, method: :get %>

<div class='movies'>
  <% @facade.movies_index.each do |movie| %>
    <div class='title'><h4><%= link_to "#{movie.title}", movie_path(movie.id) %></h4></div>
    <div class='movie-vote-average'><h4>Vote Average <%= movie.vote_average %></h4></div>
  <% end %>
</div>